{
  "name": "@stoui/core",
  "version": "0.1.0",
  "description": "Stone UI Library for React",
  "scripts": {
    "storybook": "start-storybook -p 9600 -c .storybook",
    "build": "cross-env NODE_ENV=production rollup --config rollup.config.js",
    "build:docs": "build-storybook -c .storybook -o .out",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build",
    "type-check": "tsc -p tsconfig.json --pretty --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "npm run lint -- --fix && npm run prettier -- --write",
    "prettier": "prettier \"**/*.{js,jsx,ts,tsx,md,css,scss,yaml,yml}\"",
    "generate": "node utils/generate",
    "commit": "git-cz"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "typings": "dist/index.d.ts",
  "tags": [
    "stoui",
    "react",
    "framework"
  ],
  "keywords": [
    "react",
    "react-ui",
    "react-component",
    "design",
    "yonui",
    "stoui",
    "library",
    "component",
    "components",
    "ui",
    "framework",
    "frontend",
    "web"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@fullhuman/postcss-purgecss": "3.1.3",
    "@rollup/plugin-commonjs": "17.0.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "11.0.1",
    "@storybook/addon-actions": "6.1.11",
    "@storybook/addon-essentials": "6.1.11",
    "@storybook/addon-links": "6.1.11",
    "@storybook/react": "6.1.11",
    "@testing-library/jest-dom": "5.11.6",
    "@testing-library/react": "11.2.2",
    "@types/jest": "26.0.19",
    "@types/react": "16.9.53",
    "@types/react-dom": "17.0.0",
    "autoprefixer": "10.1.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "babel-preset-react-app": "10.0.0",
    "clsx": "1.1.1",
    "cross-env": "7.0.3",
    "eslint": "7.16.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-prettier": "3.3.0",
    "eslint-plugin-react": "7.21.5",
    "husky": "4.3.6",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "lint-staged": "10.5.3",
    "node-sass": "5.0.0",
    "postcss": "8.2.1",
    "postcss-import": "14.0.0",
    "postcss-loader": "4.1.0",
    "postcss-nested": "5.0.3",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.2.1",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-is": "17.0.1",
    "rollup": "2.35.1",
    "rollup-plugin-copy": "3.3.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-postcss": "4.0.0",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.29.0",
    "sass-loader": "10.1.0",
    "standard-version": "9.0.0",
    "styled-components": "5.2.1",
    "tailwindcss": "2.0.2",
    "ts-jest": "26.4.4",
    "typescript": "4.1.3"
  },
  "peerDependencies": {
    "@types/react": ">=17.0.0",
    "react": ">=17.0.1",
    "react-dom": ">=17.0.1"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": "https://github.com/haiyon/stoui.git",
  "author": "Youxun Interaction Team",
  "files": [
    "dist",
    "types",
    "README.md",
    "LICENSE"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test",
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "license": "MIT"
}
