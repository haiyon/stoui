{
  "name": "@stoui/react",
  "version": "0.0.1",
  "description": "React components that implement Stone UI",
  "main": "index.js",
  "module": "index.es.js",
  "scripts": {
    "clean:dist": "rimraf dist",
    "storybook": "start-storybook -p 9600 -c .storybook",
    "build": "pnpm run clean:dist && cross-env NODE_ENV=production rollup --config rollup.config.js --bundleConfigAsCjs && pnpm run copy",
    "build:docs": "build-storybook -c .storybook -o .out",
    "copy": "cpy package.json README.md LICENSE dist/",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "release": "standard-version",
    "type-check": "tsc -p tsconfig.json --pretty --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "pnpm run lint --fix && pnpm run prettier --write",
    "prettier": "prettier \"**/*.{js,jsx,ts,tsx,md,css,scss,yaml,yml}\"",
    "generate": "node utils/generate",
    "commit": "git-cz"
  },
  "tags": [
    "stoui",
    "react",
    "framework"
  ],
  "keywords": [
    "react",
    "react-ui",
    "react-component",
    "design",
    "yonui",
    "stoui",
    "library",
    "component",
    "components",
    "ui",
    "framework",
    "frontend",
    "web"
  ],
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@storybook/addon-a11y": "^6.5.16",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addons": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/theming": "^6.5.16",
    "@stoui/utils": "*",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@types/bluebird": "^3.5.38",
    "@types/jest": "^29.4.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "babel-preset-react-app": "^10.0.1",
    "clsx": "^1.2.1",
    "cpy-cli": "^4.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.8.0",
    "react-is": "^18.2.0",
    "react-markdown": "^8.0.5",
    "rimraf": "^4.4.0",
    "rollup": "^3.19.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "standard-version": "^9.5.0",
    "storybook-dark-mode": "^2.1.1",
    "styled-components": "^5.3.8",
    "tailwindcss": "^3.2.7",
    "ts-jest": "^29.0.5",
    "ts-toolbelt": "^9.6.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "@types/react": ">=17.0.15",
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "publishConfig": {
    "registry": "https://registry.pnpmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haiyon/stoui.git",
    "directory": "packages/stoui-react"
  },
  "bugs": {
    "url": "https://github.com/haiyon/stoui/issues"
  },
  "author": "Shen <hi@haiyon.com>",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "pnpm run format"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "pnpm run test",
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "license": "MIT"
}
