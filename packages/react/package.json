{
  "name": "@stoui/react",
  "version": "0.0.1",
  "description": "React components that implement Stone UI",
  "scripts": {
    "storybook": "start-storybook -p 9600 -c .storybook",
    "build": "rimraf dist && cross-env NODE_ENV=production rollup --config rollup.config.js && npm run copy",
    "build:docs": "build-storybook -c .storybook -o .out",
    "copy": "cpy package.json README.md LICENSE dist/",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "release": "standard-version",
    "type-check": "tsc -p tsconfig.json --pretty --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "npm run lint -- --fix && npm run prettier -- --write",
    "prettier": "prettier \"**/*.{js,jsx,ts,tsx,md,css,scss,yaml,yml}\"",
    "generate": "node utils/generate",
    "commit": "git-cz"
  },
  "main": "index.js",
  "module": "index.es.js",
  "tags": [
    "stoui",
    "react",
    "framework"
  ],
  "keywords": [
    "react",
    "react-ui",
    "react-component",
    "design",
    "yonui",
    "stoui",
    "library",
    "component",
    "components",
    "ui",
    "framework",
    "frontend",
    "web"
  ],
  "devDependencies": {
    "@stoui/utils": "workspace:*",
    "@babel/core": "^7.16.5",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "@storybook/addon-a11y": "^6.4.22",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addons": "^6.4.22",
    "@storybook/builder-webpack5": "^6.5.8",
    "@storybook/manager-webpack5": "^6.5.8",
    "@storybook/react": "^6.4.22",
    "@storybook/theming": "^6.4.22",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^13.3.0",
    "@types/bluebird": "^3.5.36",
    "@types/jest": "^28.1.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.42",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "babel-preset-react-app": "^10.0.1",
    "clsx": "^1.1.1",
    "cpy-cli": "^4.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "lint-staged": "^13.0.1",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "prop-types": "^15.8.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-is": "^18.1.0",
    "react-markdown": "^8.0.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.62.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "standard-version": "^9.3.2",
    "storybook-dark-mode": "^1.0.9",
    "styled-components": "^5.3.3",
    "tailwindcss": "^3.0.7",
    "ts-jest": "^28.0.4",
    "ts-toolbelt": "^9.6.0",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "@types/react": ">=17.0.15",
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haiyon/stoui.git",
    "directory": "packages/stoui-react"
  },
  "bugs": {
    "url": "https://github.com/haiyon/stoui/issues"
  },
  "author": "Shen <hi@haiyon.com>",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run format"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test",
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "license": "MIT"
}
