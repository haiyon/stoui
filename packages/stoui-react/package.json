{
  "name": "@stoui/react",
  "version": "0.0.1",
  "description": "React components that implement Stone UI",
  "scripts": {
    "storybook": "start-storybook -p 9600 -c .storybook",
    "build": "rimraf dist && cross-env NODE_ENV=production rollup --config rollup.config.js",
    "build:docs": "build-storybook -c .storybook -o .out",
    "copy": "cpy package.json README.md LICENSE dist/",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "release": "standard-version",
    "prepublishOnly": "npm run build && npm run copy",
    "type-check": "tsc -p tsconfig.json --pretty --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "npm run lint -- --fix && npm run prettier -- --write",
    "prettier": "prettier \"**/*.{js,jsx,ts,tsx,md,css,scss,yaml,yml}\"",
    "generate": "node utils/generate",
    "commit": "git-cz"
  },
  "main": "index.js",
  "tags": [
    "stoui",
    "react",
    "framework"
  ],
  "keywords": [
    "react",
    "react-ui",
    "react-component",
    "design",
    "yonui",
    "stoui",
    "library",
    "component",
    "components",
    "ui",
    "framework",
    "frontend",
    "web"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@rollup/plugin-commonjs": "19.0.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.0.0",
    "@storybook/addon-a11y": "6.3.4",
    "@storybook/addon-actions": "6.3.4",
    "@storybook/addon-essentials": "6.3.4",
    "@storybook/addon-info": "5.3.21",
    "@storybook/addon-links": "6.3.4",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/addons": "6.3.4",
    "@storybook/react": "6.3.4",
    "@storybook/theming": "6.3.4",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "@types/jest": "26.0.24",
    "@types/react": "17.0.14",
    "@types/react-dom": "17.0.9",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "autoprefixer": "10.2.6",
    "babel-jest": "27.0.6",
    "babel-loader": "8.2.2",
    "babel-preset-react-app": "10.0.0",
    "clsx": "1.1.1",
    "cpy-cli": "3.1.1",
    "cross-env": "7.0.3",
    "eslint": "7.30.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "husky": "7.0.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.0.6",
    "lint-staged": "11.0.0",
    "postcss": "8.3.5",
    "postcss-import": "14.0.2",
    "postcss-loader": "4.3.0",
    "postcss-nested": "5.0.5",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.3.2",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "17.0.2",
    "rimraf": "3.0.2",
    "rollup": "2.53.0",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-postcss": "4.0.0",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.30.0",
    "sass": "1.35.2",
    "sass-loader": "10.2.0",
    "standard-version": "9.3.0",
    "storybook-dark-mode": "1.0.8",
    "styled-components": "5.3.0",
    "tailwindcss": "2.2.4",
    "ts-jest": "27.0.3",
    "typescript": "4.3.5"
  },
  "peerDependencies": {
    "@types/react": ">=17.0.14",
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": "https://github.com/haiyon/stoui.git",
  "author": {
    "name": "Shen",
    "url": "https://haiyon.com",
    "email": "hi@haiyon.com"
  },
  "maintainers": [
    {
      "name": "Shen",
      "url": "https://haiyon.com",
      "email": "hi@haiyon.com"
    }
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run format"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test",
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "license": "MIT"
}
